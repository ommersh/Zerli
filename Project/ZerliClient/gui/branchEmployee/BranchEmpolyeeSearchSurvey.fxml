<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="branchEmployee.SearchSurverControllerGUI">
   <children>
      <Pane fx:id="searchSurveyPane" prefHeight="695.0" prefWidth="1200.0">
         <children>
            <Button fx:id="SearchButton" layoutX="548.0" layoutY="446.0" mnemonicParsing="false" onAction="#searchSurvey" style="-fx-background-color: #2f41a7;" text="search" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="22.0" />
               </font>
            </Button>
            <TextField fx:id="searchBox" layoutX="477.0" layoutY="252.0" prefHeight="56.0" prefWidth="239.0" />
            <Label layoutX="484.0" layoutY="186.0" prefHeight="45.0" prefWidth="224.0" text="Search Survey:">
               <font>
                  <Font name="System Bold Italic" size="30.0" />
               </font>
            </Label>
            <Label fx:id="Error_label" contentDisplay="CENTER" layoutX="270.0" layoutY="362.0" prefHeight="61.0" prefWidth="660.0" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Label>
            <Label layoutX="223.0" layoutY="267.0" text="Please enter a survey number:">
               <font>
                  <Font name="System Bold Italic" size="17.0" />
               </font>
            </Label>
            <Button fx:id="PrevPageButton" layoutX="55.0" layoutY="594.0" mnemonicParsing="false" onAction="#goBack" prefHeight="45.0" prefWidth="120.0" style="-fx-background-color: #2f41a7;" text="Back" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</VBox>
