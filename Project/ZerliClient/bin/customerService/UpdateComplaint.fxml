<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="basePane" prefHeight="695.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customerService.UpdateComplaint">
   <children>
      <Label fx:id="answerLabel" layoutX="150.0" layoutY="216.0" text="answer">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Label>
      <TextField fx:id="answerText" alignment="TOP_LEFT" layoutX="311.0" layoutY="211.0" prefHeight="71.0" prefWidth="600.0" promptText="enter your amswer to the client" />
      <Label fx:id="compensationLabel" layoutX="153.0" layoutY="359.0" text="compensation">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Label>
      <TextField fx:id="compensationText" alignment="TOP_LEFT" layoutX="311.0" layoutY="359.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="36.0" prefWidth="141.0" promptText="enter compensation" />
      <Button fx:id="updateBtn" layoutX="533.0" layoutY="608.0" mnemonicParsing="false" onAction="#updateComplaint" style="-fx-background-color: #2f41a7;" text="update" textFill="#efe7e7">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <Label fx:id="msgLabel" alignment="CENTER" layoutX="154.0" layoutY="491.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="77.0" prefWidth="841.0" textAlignment="CENTER" textFill="#ec0606">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutX="50.0" layoutY="608.0" mnemonicParsing="false" onAction="#BackToShowAllComplaintsWindow" prefHeight="34.0" prefWidth="86.0" style="-fx-background-color: #2f41a7;" text="back" textFill="#faf8f8">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
