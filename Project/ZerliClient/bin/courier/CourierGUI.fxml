<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="courier.CourierControllerGUI">
   <children>
      <Pane fx:id="DeliveryPane" prefHeight="695.0" prefWidth="1200.0">
         <children>
            <TextField fx:id="orderNum_txt" layoutX="442.0" layoutY="270.0" onAction="#enterOrderNumber" prefHeight="54.0" prefWidth="368.0" />
            <Label layoutX="61.0" layoutY="265.0" prefHeight="60.0" prefWidth="355.0" text="Please enter an order number:">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Label>
            <Label layoutX="460.0" layoutY="177.0" text="Shipment verification">
               <font>
                  <Font name="System Bold Italic" size="33.0" />
               </font>
            </Label>
            <Button fx:id="confirmDeilveryButton" layoutX="851.0" layoutY="269.0" mnemonicParsing="false" onAction="#ConfirmDelivery" style="-fx-background-color: #2f41a7;" text="Confirm Delivery" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="22.0" />
               </font>
            </Button>
            <Label fx:id="erorrLabel" alignment="CENTER" layoutX="338.0" layoutY="369.0" prefHeight="102.0" prefWidth="734.0" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="27.0" />
               </font>
            </Label>
            <Button fx:id="PrevPageButton" layoutX="80.0" layoutY="614.0" mnemonicParsing="false" onAction="#backToMainPage" prefHeight="45.0" prefWidth="120.0" style="-fx-background-color: #2f41a7;" text="Back" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="22.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</VBox>
